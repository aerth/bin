#!/bin/sh

if [ -f main.go ]; then
echo "main.go already exists."
exit 1
fi


cat >> main.go <<'EOF'
package main

/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
*    
*   Copyright (C) 2017 aerth <aerth@riseup.net
